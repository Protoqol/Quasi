{
    "version": "1.0.1",
    "name": "protoqol/quasi",
    "description": "Quasi",
    "keywords": [
        "protoqol",
        "quasi"
    ],
    "homepage": "https://github.com/protoqol/quasi",
    "license": "GPL-3.0-only",
    "type": "library",
    "authors": [
        {
            "name": "Quinten Justus",
            "email": "quinten@protoqol.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Protoqol\\Quasi\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Protoqol\\Quasi\\QuasiServiceProvider"
            ]
        }
    }
}
